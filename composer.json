	"require-dev": {
		"browserstack/browserstack-local": "^1.1",
		"fakerphp/faker": "1.23.1",
		"php-webdriver/webdriver": "1.15.1",
		"phpstan/phpstan": "1.11.8",
		"phpunit/phpunit": "9.6.20",
		"phpunit/phpunit-selenium": "9.0.1"
	}
}
